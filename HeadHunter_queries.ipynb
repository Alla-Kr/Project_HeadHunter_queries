      "outputs": [],
      "source": [
        "query_5_7 = f'''(select \n",
        "                    a.name \"Город\",\n",
        "                    count(v.id) \n",
        "                from vacancies v\n",
        "                join employers e on e.id = v.employer_id\n",
        "                join areas a on a.id = v.area_id\n",
        "                where\n",
        "                e.name = 'Яндекс'\n",
        "                and a.name in {town_1}\n",
        "                group by a.name)\n",
        "\n",
        "                union \n",
        "\n",
        "                (select \n",
        "                    'Total',\n",
        "                    count(v.id)\n",
        "                from vacancies v\n",
        "                join employers e on e.id = v.employer_id\n",
        "                join areas a on a.id = v.area_id\n",
        "                where\n",
        "                e.name = 'Яндекс'\n",
        "                and a.name in {town_1})\n",
        "                order by 2 \n",
        "            '''\n"
      ]
